{
  "segments": [
    "virtual_env",
    "username",
    "hostname",
    "aws_profile",
    "ssh",
    "cwd",
    "git",
    "git_stash",
    "jobs",
    "set_term_title",
    "svn",
    "newline",
    "root"
  ],
  "mode": "patched",
  "cwd": {
    "mode": "plain",
    "max_depth": 4
  },
  "theme": "~/.config/powerline-shell/themes/my_theme.py"
}

